  "description": "Sistema de mapeo de redes hipergráficas con rigor teórico para generar hipergrafos persistentes a partir de redes neuronales",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "run_simulacion": "npx ts-node src/run_simulacion.ts",
    "run_entrenamiento": "npx ts-node src/run_entrenamiento.ts",
    "run_estres": "npx ts-node src/run_estres.ts",
    "run_omnisciente": "npx ts-node src/run_omnisciente.ts",
    "simular_cognicion": "npx ts-node src/simular_cognicion.ts",
    "enviar_dataset": "npx ts-node src/enviar_dataset.ts"
