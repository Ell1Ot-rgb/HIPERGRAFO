    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/express": "^5.0.6",
    "axios": "^1.13.2",
    "express": "^5.2.1",
    "onnxruntime-node": "^1.23.2",
    "uuid": "^9.0.0"
  }
